post_upgrade() {
    if [[ "$(vercmp "$2" 3.5.0)" != "1" ]] && [[ "$(vercmp "$1" 3.5.0)" != "-1" ]]
    then
        echo "System backends are supported since 3.5.0, please see upstream release note for more info."
    fi
}

post_install() {
    echo "Backends are splitted into several packages. Please select which you need."
}
